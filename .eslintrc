{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "env": {
    "browser": true,
    "jest": true
  },
  "plugins": [
    "react",
    "import"
  ],
  "rules": {
    "react/jsx-filename-extension": [
      "error",
      {
        "extensions": [
          ".js",
          ".jsx"
        ]
      }
    ],
    "no-plusplus": [
      2,
      {
        "allowForLoopAfterthoughts": true
      }
    ],
    "react/jsx-indent": [
      4,
      4
    ],
    "react/jsx-indent-props": [
      "error",
      "tab"
    ],
    "jsx-quotes": [
      "error",
      "prefer-single"
    ],
    "no-tabs": "off",
    "indent": [
      "error",
      "tab"
    ],
    "required": {
      "some": [
        "nesting",
        "id"
      ]
    },
    "import/prefer-default-export": "off",
    "no-unused-expressions": "off",
    "jsx-a11y/label-has-for": "off",
    "no-alert": "off",
    "jsx-a11y/label-has-associated-control": [
      2,
      {
        "controlComponents": [
          "InputCheckbox"
        ],
        "depth": 3,
      }
    ],
  }
}